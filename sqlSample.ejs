<!DOCTYPE html>
<html lang="ja">

<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <title>
        <%= title %>
    </title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
    <h1>
        <%= title %>
    </h1>
    <div role="main">
        <table>
            <tr>
                <th>製品コード</th>
                <th>製品名</th>
                <th>単価</th>
            </tr>
            <% content.forEach(function (value, key) { %>
                <tr>
                    <td>
                        <%= value.ProductsCode %>
                    </td>
                    <td>
                        <%= value.ProductsName %>
                    </td>
                    <td>
                        <%= value.UnitPrice %>
                    </td>
                </tr>
                <% }); %>
        </table>
    </div>
</body>

</html>